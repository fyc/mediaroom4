package com.sunmnet.mediaroom.brand.control.table;

/**
 * 课程表行列方向属性
 * COURSE_HEADER_COL:课节数据排列显示为一列
 * COURSE_HEADER_ROW:课节数据排列显示为一行
 */
public enum CourseTableDirection {

    COURSE_HEADER_COL("courseHeaderCol"), COURSE_HEADER_ROW("courseHeaderRow");

    private String directionValue;

    CourseTableDirection(String value) {
        this.directionValue = value;
    }

    public String getValue() {
        return directionValue;
    }
}
