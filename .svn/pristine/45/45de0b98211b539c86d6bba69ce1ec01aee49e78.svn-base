<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data class="SettingItemMenu">

        <import type="com.sunmnet.mediaroom.tabsp.bean.MenuEntity" />

        <variable
            name="menu"
            type="MenuEntity" />
    </data>

    <TextView
        android:id="@+id/setting_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/px_35"
        android:paddingTop="@dimen/px_14"
        android:paddingRight="@dimen/px_35"
        android:paddingBottom="@dimen/px_14"
        android:text="@{menu.menuName}">

    </TextView>
</layout>
