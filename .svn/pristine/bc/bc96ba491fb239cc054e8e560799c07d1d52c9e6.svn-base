package com.sunmnet.mediaroom.brand.control.media;

import android.content.Context;
import android.support.annotation.Nullable;
import android.util.AttributeSet;

import com.sunmnet.mediaroom.brand.R;

/**
 * 老师头像
 */
public class TemplateTeacherPortraitControl extends TeacherPortraitControl {

    public TemplateTeacherPortraitControl(Context context) {
        super(context);
    }

    public TemplateTeacherPortraitControl(Context context, MarginLayoutParams layoutParams) {
        super(context, layoutParams);
    }

    public TemplateTeacherPortraitControl(Context context, @Nullable AttributeSet attrs) {
        super(context, attrs);
    }

    @Override
    protected void init() {
        defaultImgResId = R.drawable.template_default_portrait;
        portrait = "self";
        super.init();
    }
}
