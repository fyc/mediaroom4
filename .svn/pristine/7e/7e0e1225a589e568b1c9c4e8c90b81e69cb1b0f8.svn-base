package com.sunmnet.mediaroom.tabsp.record.interfaces;


public interface IRecordPlayer {
    /**
     * 与主机通信正常后回调
     * */
    public void onReady();
    /**
     * 开始播放录播画面
     * */
    public void start();
    /**
     * 停止播放画面
     * */
    public void stop();
    /**
     * 暂停播放画面
     * */
    public void pause();
    /**
     * 回收资源
     * */
    public void release();
}
