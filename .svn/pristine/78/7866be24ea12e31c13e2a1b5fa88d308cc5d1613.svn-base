package com.sunmnet.mediaroom.brand.bean.play;

import java.io.Serializable;

public class TemplateProgramRule extends PublishingRule implements Serializable {

    private TemplateProgramInfo resource;
    private Integer templateType;

    public TemplateProgramInfo getResource() {
        return resource;
    }

    public void setResource(TemplateProgramInfo resource) {
        this.resource = resource;
    }

    public Integer getTemplateType() {
        return templateType;
    }

    public void setTemplateType(Integer templateType) {
        this.templateType = templateType;
    }
}
