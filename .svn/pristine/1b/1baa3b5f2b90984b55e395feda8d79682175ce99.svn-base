<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <stroke
                android:width="1px"
                android:color="@color/colorPrimary" />
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
    <item android:bottom="1px">
        <shape>
            <stroke
                android:width="1px"
                android:color="@color/colorPrimary"
                android:dashWidth="2px"
                android:dashGap="2px" />
            <solid android:color="#FFFFFF" />
        </shape>
    </item>

</layer-list>