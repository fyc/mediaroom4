package com.sunmnet.mediaroom.brand.bean.control.text;

import com.sunmnet.mediaroom.brand.bean.control.base.PrefixSuffixTextControlAttr;

public class ClassNoControlAttr extends PrefixSuffixTextControlAttr {
    /**
     * 匹配课节，0为自动匹配，其余为匹配对应课节，双课节用逗号隔开
     */
    String classNo;

    /**
     * 0:中文;1阿拉伯数字
     */
    String showType;

    /**
     * 1 单课节 2双课节
     */
    int classType;

    /**
     * 双课节连接符
     */
    String connect;


    public String getClassNo() {
        return classNo;
    }

    public void setClassNo(String classNo) {
        this.classNo = classNo;
    }

    public String getShowType() {
        return showType;
    }

    public void setShowType(String showType) {
        this.showType = showType;
    }

    public int getClassType() {
        return classType;
    }

    public void setClassType(int classType) {
        this.classType = classType;
    }

    public String getConnect() {
        return connect;
    }

    public void setConnect(String connect) {
        this.connect = connect;
    }
}
