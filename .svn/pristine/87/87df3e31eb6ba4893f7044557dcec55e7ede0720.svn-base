package com.sunmnet.mediaroom.brand.bean.item;

import com.bin.david.form.annotation.SmartColumn;
import com.bin.david.form.annotation.SmartTable;

@SmartTable(name = "考场查询结果")
public class QueryExamScoreResultItem {

    @SmartColumn(id = 1, name = "科目")
    private String subject1;

    @SmartColumn(id = 2, name = "成绩")
    private String score1;

    @SmartColumn(id = 3, name = "科目")
    private String subject2;

    @SmartColumn(id = 4, name = "成绩")
    private String score2;

    public QueryExamScoreResultItem() {
    }

    public QueryExamScoreResultItem(String subject1, String score1, String subject2, String score2) {
        this.subject1 = subject1;
        this.score1 = score1;
        this.subject2 = subject2;
        this.score2 = score2;
    }

    public String getSubject1() {
        return subject1;
    }

    public void setSubject1(String subject1) {
        this.subject1 = subject1;
    }

    public String getScore1() {
        return score1;
    }

    public void setScore1(String score1) {
        this.score1 = score1;
    }

    public String getSubject2() {
        return subject2;
    }

    public void setSubject2(String subject2) {
        this.subject2 = subject2;
    }

    public String getScore2() {
        return score2;
    }

    public void setScore2(String score2) {
        this.score2 = score2;
    }
}
