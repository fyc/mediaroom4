<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data class="SettingItemMenu">

        <import type="com.sunmnet.mediaroom.tabsp.bean.MenuEntity" />

        <variable
            name="menu"
            type="MenuEntity" />
    </data>

    <TextView
        android:id="@+id/setting_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/px_14"
        android:paddingTop="@dimen/px_14"
        android:paddingRight="@dimen/px_14"
        android:paddingBottom="@dimen/px_14"
        android:text="@{menu.menuName}"
        tools:text="网络配置"
        tools:textColor="@color/color_477ED7">

    </TextView>
</layout>
